<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_home"
    app:startDestination="@id/home2">
    <fragment
        android:id="@+id/home2"
        android:name="com.minorproject.cloudgallery.screens.home.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_home2_to_userProfile"
            app:destination="@id/userProfile"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
        <action
            android:id="@+id/action_home2_to_timeline2"
            app:destination="@id/timeline2"
            app:enterAnim="@anim/return_enter"
            app:exitAnim="@anim/return_exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
    </fragment>
    <fragment
        android:id="@+id/timeline2"
        android:name="com.minorproject.cloudgallery.screens.timeline.Timeline"
        android:label="fragment_timeline"
        tools:layout="@layout/fragment_timeline">
        <action
            android:id="@+id/action_timeline2_to_home2"
            app:destination="@id/home2"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
        <action
            android:id="@+id/action_timeline2_to_userProfile"
            app:destination="@id/userProfile"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
    </fragment>
    <fragment
        android:id="@+id/userProfile"
        android:name="com.minorproject.cloudgallery.screens.profile.UserProfile"
        android:label="fragment_user_profile"
        tools:layout="@layout/fragment_user_profile">
        <action
            android:id="@+id/action_userProfile_to_home2"
            app:destination="@id/home2"
            app:enterAnim="@anim/return_enter"
            app:exitAnim="@anim/return_exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
        <action
            android:id="@+id/action_userProfile_to_timeline2"
            app:destination="@id/timeline2"
            app:enterAnim="@anim/return_enter"
            app:exitAnim="@anim/return_exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
    </fragment>
</navigation>